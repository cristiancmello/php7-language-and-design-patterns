{
  "name": "cristiancmello/design-patterns",
  "description": "DesignPatterns examples",
  "authors": [
    {
      "name": "Cristian Mello",
      "email": "cristianc.mello@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.2.0",
    "damianopetrungaro/clean-architecture": "^0.4.0",
    "guzzlehttp/guzzle": "^6.3",
    "ramsey/uuid": "^3.8",
    "squizlabs/php_codesniffer": "^3.3",
    "zendframework/zend-hydrator": "^2.4"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  },
  "autoload": {
    "psr-4": {
      "DesignPatterns\\": "src/"
    }
  }
}
